<h1>User</h1>
<div class="text-center">
<div class ="btn-group gap-2 col-md-2  mx-3 " >
<ul>
  <% @users.each do |user| %>
    <table>
  <thead>
  <li>
        <%= user.email %> 
        </li>
        </thead>
        <tbody>
        <tr>
        <td><%=link_to "Edit User", edit_user_path(user) , class:"btn btn-dark";%> </td>
        <td><%= link_to "Delete", user_path(user), method: :delete, data: { confirm: "Do you want to delete this user?" } , class:"btn btn-dark";%></li> </td>
        <td><%=link_to "User's Notes", "/users/#{user.id}/notes", class:"btn btn-dark"; %></td>
        <td><%=link_to "User's Collections", "/users/#{user.id}/collections" , class:"btn btn-dark";%></td>
        </tr>

        </tbody>
        </table>
  <% end %>
</ul>
</div>
</div>
<div class="text-center">
<div class ="btn-group gap-2 col-md-2  mx-3 " >
<%=link_to "New Note", new_note_path, class:"btn btn-dark";%>
</div>
</div>
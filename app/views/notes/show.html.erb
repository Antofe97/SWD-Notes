<h1> <%= @note.title %> </h1>

<p> <%= @note.body %> </p>

<img src="<%= @note.image %>">

<ul>
  <li><%= link_to "Edit", edit_note_path(@note) %></li>
  <li><%= link_to "Delete", note_path(@note),
                  method: :delete,
                  data: { confirm: "Do you want to delete this note?" } %></li>
</ul>